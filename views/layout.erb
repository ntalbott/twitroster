<html>
  <head>
    <title>TwitRoster</title>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.3.2/jquery.js"></script>
    <script type="text/javascript">
      jQuery(function($) {
        $('#more_users').show().click(function() {
          for(i=0; i<5; i++) {
            $('#user_input_prototype').clone().show().appendTo('#users');
          }
          return false;
        });
      });
    </script>
  </head>
  <body>
    <%= yield %>
  </body>
</html>